{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:recommended',
  ],
  enabled: true,
  dependencyDashboard: true,
  dependencyDashboardTitle: 'Renovate Dashboard ðŸ¤–',
  baseBranches: [
    'main',
  ],
  customManagers: [
    {
      customType: 'regex',
      fileMatch: [
        '^compose.+\\.ya?ml$',
      ],
      matchStrings: [
        'image: (?<depName>.*?):\\${(?:.*):?-(?<currentValue>.*?)}',
      ],
      datasourceTemplate: 'docker',
    },
  ],
  separateMajorMinor: true,
}
